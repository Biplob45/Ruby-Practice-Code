array = [1,2,3,"Biplob",[4,2,4,"Orton"]]
puts "Print All:"
puts array

puts "Print the number three Position: "
puts array[3]

puts "Print the number four Position:  "
puts array[4]

puts "Biplob added in the array list: "
puts array.include?("Biplob")

puts "Is Orton Added in the array list: "
puts array.include?("Orton")

puts "Print the array in reverse format: "
puts array.reverse

puts "Print the array list with shuffle: "
puts array.shuffle

puts "Create an array 0 to 10: "
puts (0..10).to_a

puts "Create an array 0 to 10 and print them with shuffle: "
puts (0..10).to_a.shuffle

puts "Push a new item in the array: "
puts "#{array.push("Orton")}"

puts "Pop a item from the array: "
puts "#{array.pop(1)}"


